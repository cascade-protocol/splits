{
	"name": "@cascade-fyi/tabs-sdk",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"description": "Codama-generated client for Squads Smart Account Program",
	"scripts": {
		"generate": "tsx scripts/generate-clients.ts",
		"build": "pnpm generate && tsdown",
		"type-check": "tsc --noEmit",
		"check": "pnpm type-check && biome check --write"
	},
	"dependencies": {},
	"devDependencies": {
		"@codama/nodes-from-anchor": "^1.3.4",
		"@codama/renderers-js": "^1.5.1",
		"@codama/visitors": "^1.4.2",
		"codama": "^1.4.2",
		"tsdown": "^0.16.6",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@solana/kit": ">=2.0.0"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"sideEffects": false,
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./generated": {
			"import": {
				"types": "./dist/generated.d.mts",
				"default": "./dist/generated.mjs"
			},
			"require": {
				"types": "./dist/generated.d.cts",
				"default": "./dist/generated.cjs"
			}
		}
	},
	"files": [
		"dist/**",
		"idl.json"
	]
}
